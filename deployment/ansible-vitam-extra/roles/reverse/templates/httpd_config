    {% if groups['hosts-kibana-log']|length >0 %}
    # Kibana-log

    ProxyPass /kibana_log/api/status http://{{hostvars[groups['hosts-kibana-log'][0]]['ip_service']}}:{{ kibana.log.port }}/api/status {{reverse_connection_short_params}}
    ProxyPassReverse /kibana_log/api/status http://{{hostvars[groups['hosts-kibana-log'][0]]['ip_service']}}:{{ kibana.log.port }}/api/status

    # Needed as it conflicts with Elasticsearch reverse proxy
    ProxyPass /kibana_log/elasticsearch http://{{hostvars[groups['hosts-kibana-log'][0]]['ip_service']}}:{{ kibana.log.port }}/elasticsearch {{reverse_connection_short_params}}
    ProxyPassReverse /kibana_log/elasticsearch http://{{hostvars[groups['hosts-kibana-log'][0]]['ip_service']}}:{{kibana.log.port}}/elasticsearch

    ProxyPass /kibana_log/app/kibana http://{{hostvars[groups['hosts-kibana-log'][0]]['ip_service']}}:{{ kibana.log.port }}/app/kibana {{reverse_connection_short_params}}
    ProxyPassReverse /kibana_log/app/kibana http://{{hostvars[groups['hosts-kibana-log'][0]]['ip_service']}}:{{ kibana.log.port }}/app/kibana

    ProxyPass /kibana_log/ http://{{hostvars[groups['hosts-kibana-log'][0]]['ip_service']}}:{{ kibana.log.port }}/ {{reverse_connection_short_params}}
    ProxyPassReverse /kibana_log http://{{hostvars[groups['hosts-kibana-log'][0]]['ip_service']}}:{{ kibana.log.port }}/
    {% endif %}

