{
  "service": {
    "name": "cerebro",
    "port": {{ cerebro.port }},
    "address": "{{ ip_service }}",
    "enable_tag_override": false,
    "checks": [
      {
        "name": "cerebro : HTTP check",
        "notes": "HTTP status check",
        "http": "http://{{ ip_service }}:{{ cerebro.port }}/{{ cerebro.baseuri }}/",
        "interval": "1s"
      }
    ]
  }
}
