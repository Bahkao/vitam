{
  "service": {
    "name": "{{ kibana_service_name }}",
    "port": {{ composant.port }},
    "address": "{{ ip_service }}",
    "enableTagOverride": false,
    "checks": [
      {
        "name": "{{ kibana_service_name }} : HTTP check",
        "notes": "Kibana home page access",
        "http": "http://{{ ip_service }}:{{ composant.port }}/app/kibana",
        "interval": "1s"
      }
    ]
  }
}
