{
  "service": {
    "name": "logstash",
    "port": {{ logstash.port }},
    "address": "{{ ip_admin }}",
    "enableTagOverride": false,
    "checks": [
      {
        "id": "syslog tcp check",
        "notes": "Syslog tcp port opened",
        "tcp": "{{ ip_admin }}:{{ logstash.port }}",
        "interval": "1s"
      }
    ]
  }
}
