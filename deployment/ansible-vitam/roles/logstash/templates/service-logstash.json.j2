{
  "service": {
    "name": "logstash",
    "port": {{ vitam_logstash_port }},
    "address": "{{ip_admin}}",
    "enableTagOverride": false,
    "checks": [
      {
        "id": "syslog tcp check",
        "notes": "Syslog tcp port opened",
        "tcp": "{{ ip_admin }}:{{ vitam_logstash_port }}",
        "interval": "1s"
      }
    ]
  }
}
