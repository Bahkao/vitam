{
  "service": {
    "name": "mongod",
    "port": {{ mongodb.mongod_port }},
    "enableTagOverride": false,
    "checks": [
      {
        "name": "mongod : TCP check",
        "notes": "Status mongod",
        "tcp": "localhost:{{ mongodb.mongod_port }}",
        "interval": "1s"
      }
    ]
  }
}
