{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Consolidated Transformation Offer (CTO) description",
  "type": "object",
  "required": [
    "id",
    "transformation_rule_ids"
  ],
  "properties": {
    "id": {
      "description": "CTO identifier",
      "type": "string"
    },
    "transformation_rule_ids": {
      "description": "List of Transformation Rule Identifiers",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}