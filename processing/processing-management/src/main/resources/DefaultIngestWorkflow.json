{
    "id": "DefaultIngestWorkflow",
    "comment":"Default Ingest Workflow V4",
    "steps":
    [
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Contrôle global entrée (SIP)",
        "stepType":"BLOCK",
        "distribution" : {
        	"kind":"REF",
        	"element" : "SIP/manifest.xml"
        	},
        "actions":[{"action":{"actionKey": "checkSeda", "actionType": "BLOCK"}},
        		{"action":{"actionKey": "CheckVersion", "actionType": "BLOCK"}},
        		{"action":{"actionKey": "CheckObjectsNumber", "actionType": "NOBLOCK"}},
        		{"action":{"actionKey": "CheckConformity", "actionType": "BLOCK"}},
        		{"action":{"actionKey": "ExtractSeda", "actionType": "BLOCK"}}]
    },    
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Vérification de la disponibilite de l'offre de stockage",
        "stepType":"BLOCK",
        "distribution" : {
        	"kind":"REF",
        	"element" : "SIP/manifest.xml"
        	},
        "actions":[{"action":{"actionKey": "CheckStorageAvailability", "actionType": "BLOCK"} }]
    },
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Rangement des units",
        "stepType":"BLOCK",
          "distribution" : {
        	"kind":"LIST",
        	"element" : "Units"
        	},
        "actions":[{"action":{"actionKey": "IndexUnit", "actionType": "BLOCK"} }]
       
    },
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Rangement des objets",
        "stepType":"BLOCK",
        "distribution" : {
        	"kind":"LIST",
        	"element" : "ObjectGroup"
        	},
        "actions":[{"action":{"actionKey": "StoreObjectGroup", "actionType": "BLOCK"}}, 
        		{"action":{"actionKey": "IndexObjectGroup", "actionType": "BLOCK"} }]
    }]
}