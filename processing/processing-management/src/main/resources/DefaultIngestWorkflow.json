{
    "id": "DefaultIngestWorkflow",
    "comment":"Default Ingest Workflow V3",
    "steps":
    [
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Check manifest existence",
        "distribution" : {
        	"kind":"REF",
        	"element" : "SIP/manifest.xml"
        	},
        "actions":[{"action":"checkSeda"}]
    },
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Check Version",
        "distribution" : {
        	"kind":"REF",
        	"element" : "SIP/manifest.xml"
        	},
        "actions":[{"action":"CheckVersion"}]
    },
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Check Objects Number",
        "distribution" : {
        	"kind":"REF",
        	"element" : "SIP/manifest.xml"
        	},
        "actions":[{"action":"CheckObjectsNumber"}]
    },
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Extract Unit and ObjectGroup",
        "distribution" : {
        	"kind":"REF",
        	"element" : "SIP/manifest.xml"
        	},
        "actions":[{"action":"ExtractSeda"}]
    },
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Index Unit",
          "distribution" : {
        	"kind":"LIST",
        	"element" : "Units"
        	},
        "actions":[{"action":"IndexUnit"}]
       
    },
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Index ObjectGroup",
          "distribution" : {
        	"kind":"LIST",
        	"element" : "ObjectGroup"
        	},
        "actions":[{"action":"IndexObjectGroup"}]
       
    },
    {
    "workerGroupId" :"DefaultWorker",
        "stepName":"Check Digest Message",
        "distribution" : {
        	"kind":"REF",
        	"element" : "SIP/manifest.xml"
        	},
        "actions":[{"action":"CheckConformity"}]
    }
    ]
}