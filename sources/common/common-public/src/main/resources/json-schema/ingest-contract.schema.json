{
  "$schema": "http:\/\/vitam-json-schema.org\/draft-04\/schema#",
  "id": "http:\/\/example.com\/root.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "_id": {
      "type": "string"
    },
    "_tenant": {
      "type": "integer"
    },
    "_v": {
      "type": "integer"
    },
    "Name": {
      "type": "string"
    },
    "Identifier": {
      "type": "string"
    },
    "Description": {
      "type": "string"
    },
    "Status": {
      "type": "string"
    },
    "CreationDate": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    },
    "LastUpdate": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    },
    "ActivationDate": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    },
    "DeactivationDate": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    },
    "LinkParentId": {
      "type": "string"
    },
    "ArchiveProfiles": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": ["_id", "_tenant", "_v", "Identifier", "Status", "CreationDate", "LastUpdate"]
}
