{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://example.com/root.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "_id": {
      "type": "string"
    },
    "_tenant": {
      "type": "integer"
    },
    "_v": {
      "type": "integer"
    },
    "Identifier": {
      "type": "string"
    },
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "string"
    },
    "CreationDate": {
      "type": "string"
    },
    "ActionList": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "GENERATE",
          "IDENTIFY",
          "ANALYSE",
          "EXTRACT"
        ]
      }
    },
    "GriffinByFormat": {
      "type": "array"
    },
    "DefaultGriffin":{
      "type": "object"
    },
    "MetadataFilter":{
      "type": "array"
    },
    "LastUpdate": {
      "type": "string"
    },
    "Fields": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "_id",
    "_tenant",
    "_v",
    "Identifier",
    "Name"
  ]
}
