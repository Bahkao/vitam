{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://example.com/root.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "_id": {
      "type": "string"
    },
    "_v": {
      "type": "integer"
    },
    "Name": {
      "type": "string"
    },
    "Status": {
      "type": "string",
      "enum": [
        "ACTIVE",
        "INACTIVE"
      ]
    },
    "EnableControl": {
      "type": "boolean"
    },
    "Identifier": {
      "type": "string"
    },
    "SecurityProfile": {
      "type": "string"
    },
    "Permissions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "tenant": {
            "type": "integer"
          },
          "AccessContracts": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "IngestContracts": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["tenant"]
      }
    },
    "CreationDate": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    },
    "LastUpdate": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    },
    "ActivationDate": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    },
    "DeactivationDate": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    }
  },
  "required": ["_id", "_v", "Identifier", "SecurityProfile", "Status", "CreationDate", "LastUpdate", "Name", "Permissions", "EnableControl"]
}
