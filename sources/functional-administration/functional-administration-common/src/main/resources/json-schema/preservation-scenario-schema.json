{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "date-opt-time": {
      "description": "pattern: Date",
      "anyOf": [
        {
          "type": "string",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$"
        },
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "string",
          "format": "date-time-vitam"
        }
      ]
    }
  },
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "_id": {
      "type": "string"
    },
    "_tenant": {
      "type": "integer"
    },
    "_v": {
      "type": "integer"
    },
    "Identifier": {
      "type": "string"
    },
    "Name": {
      "type": "string"
    },
    "Description": {
      "type": "string"
    },
    "CreationDate": {
      "$ref": "#/definitions/date-opt-time"
    },
    "ActionList": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "GENERATE",
          "IDENTIFY",
          "ANALYSE",
          "EXTRACT",
          "EXTRACT_AU"
        ]
      }
    },
    "GriffinByFormat": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "FormatList": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "uniqueItems": true,
            "minItems": 1
          },
          "GriffinIdentifier": {
            "type": "string"
          },
          "Timeout": {
            "type": "integer",
            "minimum": 1,
            "maximum": 2147483647
          },
          "MaxSize": {
            "type": "integer",
            "minimum": 1
          },
          "Debug": {
            "type": "boolean"
          },
          "ActionDetail": {
            "type": "array",
            "items": {
              "type": "object",
              "additionalProperties": false,
              "properties": {
                "Type": {
                  "type": "string",
                  "enum": [
                    "GENERATE",
                    "IDENTIFY",
                    "ANALYSE",
                    "EXTRACT",
                    "EXTRACT_AU"
                  ]
                },
                "Values": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "Extension": {
                      "type": "string"
                    },
                    "Args": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "FilteredExtractedObjectGroupData": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "uniqueItems": true
                    },
                    "FilteredExtractedUnitData": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "uniqueItems": true
                    }
                  }
                }
              },
              "required": [
                "Type"
              ]
            }
          }
        },
        "required": [
          "FormatList",
          "GriffinIdentifier",
          "Timeout",
          "MaxSize",
          "Debug",
          "ActionDetail"
        ]
      }
    },
    "DefaultGriffin": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "GriffinIdentifier": {
          "type": "string"
        },
        "Timeout": {
          "type": "integer",
          "minimum": 1,
          "maximum": 2147483647
        },
        "MaxSize": {
          "type": "integer",
          "minimum": 1
        },
        "Debug": {
          "type": "boolean"
        },
        "ActionDetail": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
              "Type": {
                "type": "string",
                "enum": [
                  "GENERATE",
                  "IDENTIFY",
                  "ANALYSE",
                  "EXTRACT",
                  "EXTRACT_AU"
                ]
              },
              "Values": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                  "Extension": {
                    "type": "string"
                  },
                  "Args": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "FilteredExtractedObjectGroupData": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "uniqueItems": true
                  },
                  "FilteredExtractedUnitData": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "uniqueItems": true
                  }
                }
              }
            },
            "required": [
              "Type"
            ]
          }
        }
      },
      "required": [
        "GriffinIdentifier",
        "Timeout",
        "MaxSize",
        "Debug",
        "ActionDetail"
      ]
    },
    "LastUpdate": {
      "$ref": "#/definitions/date-opt-time"
    }
  },
  "required": [
    "_id",
    "_tenant",
    "_v",
    "Identifier",
    "Name"
  ]
}
