{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "_id": {
      "type": "string"
    },
    "_v": {
      "type": "integer"
    },
    "Identifier": {
      "type": "string"
    },
    "Name": {
      "type": "string"
    },
    "FullAccess": {
      "type": "boolean"
    },
    "Permissions": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "anyOf": [
    {
      "required": ["_id", "_v", "Identifier", "FullAccess", "Permissions", "Name"]
    },
    {
      "required": ["_id", "_v", "Identifier", "FullAccess", "Name"]
    }
  ]
  
}
