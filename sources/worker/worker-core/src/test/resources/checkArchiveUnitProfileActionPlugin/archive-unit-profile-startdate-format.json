{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "Management": {
      "patternProperties": {
        ".*": {
          "properties": {
            "Rules": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "StartDate": {
                    "type": "string",
                    "pattern": "^(20[0-2][0-9]-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01]))$"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
